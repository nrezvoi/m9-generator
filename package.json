{
  "name": "m9-generator",
  "version": "0.0.1",
  "description": "Static Site Generator",
  "bin": {
    "m9": "./bin/m9.js"
  },
  "scripts": {
    "build": "npm run build-dist && npm run build-example",
    "build-dist": "rm -rf dist/* && babel -s inline -d dist src",
    "build-example": "SITE=example NODE_ENV=production bin/m9.js build",
    "dev": "SITE=example bin/m9.js dev",
    "test": "npm run test-src && npm run test-example",
    "test-src": "standard --verbose src/*.js src/**/*.js | snazzy",
    "test-example": "SITE=example bin/m9.js test"
  },
  "author": "Aleksandr Tsertkov <tsertkov@gmail.com>",
  "license": "MIT",
